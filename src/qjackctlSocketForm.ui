    <function specifier="non virtual">load( qjackctlPatchbaySocket * pSocket )</function>
    <function specifier="non virtual">save( qjackctlPatchbaySocket * pSocket )</function>
    <function access="private" specifier="non virtual" returnType="bool">validateForm()</function>
</functions>
