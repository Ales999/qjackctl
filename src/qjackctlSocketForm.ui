                        </property>
                    </widget>
                    <widget class="QPushButton" row="2" column="2">
                        <property name="name">
                            <cstring>PlugAddPushButton</cstring>
                        </property>
                        <property name="text">
                            <string>&amp;Add Plug</string>
                        </property>
