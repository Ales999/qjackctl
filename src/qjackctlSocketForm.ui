                        </property>
                        <property name="text">
                            <string>&amp;Down</string>
                        </property>
                        <property name="accel">
                            <string>Alt+D</string>
                        </property>
                        <property name="iconSet">
                            <iconset>down1.png</iconset>
                        </property>
