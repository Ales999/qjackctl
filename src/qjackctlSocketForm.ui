<connections>
    <connection>
        <sender>OkPushButton</sender>
        <signal>clicked()</signal>
        <receiver>qjackctlSocketForm</receiver>
        <slot>acceptForm()</slot>
    </connection>
    <connection>
        <sender>CancelPushButton</sender>
        <signal>clicked()</signal>
        <receiver>qjackctlSocketForm</receiver>
        <slot>reject()</slot>
    </connection>
    <connection>
        <sender>PlugAddPushButton</sender>
        <signal>clicked()</signal>
        <receiver>qjackctlSocketForm</receiver>
        <slot>addPlug()</slot>
    </connection>
    <connection>
        <sender>PlugRemovePushButton</sender>
        <signal>clicked()</signal>
        <receiver>qjackctlSocketForm</receiver>
        <slot>removePlug()</slot>
    </connection>
    <connection>
        <sender>PlugUpPushButton</sender>
        <signal>clicked()</signal>
        <receiver>qjackctlSocketForm</receiver>
        <slot>moveUpPlug()</slot>
    </connection>
    <connection>
        <sender>PlugDownPushButton</sender>
        <signal>clicked()</signal>
        <receiver>qjackctlSocketForm</receiver>
        <slot>moveDownPlug()</slot>
    </connection>
    <connection>
        <sender>PlugListBox</sender>
        <signal>selectionChanged()</signal>
        <receiver>qjackctlSocketForm</receiver>
        <slot>stabilizeForm()</slot>
    </connection>
    <connection>
        <sender>SocketNameLineEdit</sender>
        <signal>textChanged(const QString&amp;)</signal>
        <receiver>qjackctlSocketForm</receiver>
        <slot>stabilizeForm()</slot>
    </connection>
    <connection>
        <sender>ClientNameComboBox</sender>
        <signal>textChanged(const QString&amp;)</signal>
        <receiver>qjackctlSocketForm</receiver>
        <slot>clientNameChanged()</slot>
    </connection>
    <connection>
        <sender>PlugNameComboBox</sender>
        <signal>textChanged(const QString&amp;)</signal>
        <receiver>qjackctlSocketForm</receiver>
        <slot>stabilizeForm()</slot>
    </connection>
</connections>
<tabstops>
    <tabstop>SocketTabWidget</tabstop>
    <tabstop>SocketNameLineEdit</tabstop>
    <tabstop>ClientNameComboBox</tabstop>
    <tabstop>PlugNameComboBox</tabstop>
    <tabstop>PlugAddPushButton</tabstop>
    <tabstop>PlugListBox</tabstop>
    <tabstop>PlugRemovePushButton</tabstop>
    <tabstop>PlugUpPushButton</tabstop>
    <tabstop>PlugDownPushButton</tabstop>
    <tabstop>OkPushButton</tabstop>
    <tabstop>CancelPushButton</tabstop>
</tabstops>
<includes>
    <include location="global" impldecl="in declaration">qjackctlPatchbay.h</include>
    <include location="local" impldecl="in implementation">qjackctlSocketForm.ui.h</include>
</includes>
<variables>
    <variable access="private">jack_client_t *m_pJackClient;</variable>
    <variable access="private">unsigned long m_ulSocketFlags;</variable>
    <variable access="private">QPixmap *m_pXpmPlug;</variable>
</variables>
<slots>
    <slot specifier="non virtual">stabilizeForm()</slot>
    <slot>acceptForm()</slot>
    <slot specifier="non virtual">addPlug()</slot>
    <slot specifier="non virtual">removePlug()</slot>
    <slot specifier="non virtual">moveUpPlug()</slot>
    <slot specifier="non virtual">moveDownPlug()</slot>
    <slot specifier="non virtual">clientNameChanged()</slot>
</slots>
<functions>
    <function access="private" specifier="non virtual">init()</function>
    <function access="private" specifier="non virtual">destroy()</function>
    <function specifier="non virtual">setSocketCaption( const QString &amp; sSocketCaption )</function>
    <function specifier="non virtual">setSocketPixmap( QPixmap *pXpmSocket )</function>
    <function specifier="non virtual">setPlugPixmap( QPixmap *pXpmPlug )</function>
