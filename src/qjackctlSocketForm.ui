    </connection>
    <connection>
        <sender>SocketNameLineEdit</sender>
        <signal>textChanged(const QString&amp;)</signal>
        <receiver>qjackctlSocketForm</receiver>
        <slot>stabilizeForm()</slot>
    </connection>
    <connection>
        <sender>SocketTypeGroup</sender>
        <signal>clicked(int)</signal>
        <receiver>qjackctlSocketForm</receiver>
        <slot>socketTypeChanged()</slot>
    </connection>
    <connection>
        <sender>ExclusiveCheckBox</sender>
        <signal>toggled(bool)</signal>
        <receiver>qjackctlSocketForm</receiver>
        <slot>socketTypeChanged()</slot>
    </connection>
    <connection>
        <sender>ClientNameComboBox</sender>
        <signal>textChanged(const QString&amp;)</signal>
        <receiver>qjackctlSocketForm</receiver>
        <slot>clientNameChanged()</slot>
    </connection>
    <connection>
        <sender>PlugNameComboBox</sender>
        <signal>textChanged(const QString&amp;)</signal>
        <receiver>qjackctlSocketForm</receiver>
        <slot>stabilizeForm()</slot>
    </connection>
    <connection>
        <sender>PlugEditPushButton</sender>
        <signal>clicked()</signal>
        <receiver>qjackctlSocketForm</receiver>
        <slot>editPlug()</slot>
    </connection>
    <connection>
        <sender>PlugListView</sender>
        <signal>contextMenuRequested(QListViewItem*,const QPoint&amp;,int)</signal>
        <receiver>qjackctlSocketForm</receiver>
        <slot>contextMenu(QListViewItem*,const QPoint&amp;,int)</slot>
    </connection>
    <connection>
        <sender>PlugListView</sender>
        <signal>itemRenamed(QListViewItem*,int)</signal>
        <receiver>qjackctlSocketForm</receiver>
        <slot>stabilizeForm()</slot>
    </connection>
</connections>
