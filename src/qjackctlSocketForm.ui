<connections>
    <connection>
        <sender>OkPushButton</sender>
        <signal>clicked()</signal>
        <receiver>qjackctlSocketForm</receiver>
        <slot>acceptForm()</slot>
    </connection>
    <connection>
        <sender>CancelPushButton</sender>
        <signal>clicked()</signal>
        <receiver>qjackctlSocketForm</receiver>
        <slot>reject()</slot>
    </connection>
    <connection>
        <sender>PlugAddPushButton</sender>
        <signal>clicked()</signal>
